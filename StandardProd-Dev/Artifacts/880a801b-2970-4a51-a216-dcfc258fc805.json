{
  "kind": "template",
  "properties": {
    "displayName": "Deploy a VNET and one subnet",
    "description": "",
    "dependsOn": [],
    "template": {
      "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
      "contentVersion": "1.0.0.0",
      "parameters": {
        "localPrefix": {
          "type": "string",
          "metadata": {
            "displayName": "Prefix for the resource names deployed in this template"
          }
        },
        "addressSpaceVnet": {
          "type": "string",
          "defaultValue": "10.0.0.0/16"
        },
        "addressSpaceSubnet": {
          "type": "string",
          "defaultValue": "10.0.0.0/24"
        }
      },
      "resources": [
        {
          "apiVersion": "2015-06-15",
          "type": "Microsoft.Network/virtualNetworks",
          "name": "[concat(parameters('localPrefix'), 'vnet-blueprint')]",
          "location": "[resourceGroup().location]",
          "properties": {
            "addressSpace": {
              "addressPrefixes": [
                "[parameters('addressSpaceVnet')]"
              ]
            },
            "subnets": [
              {
                "name": "[concat(parameters('localPrefix'), 'subnet-blueprint')]",
                "properties": {
                  "addressPrefix": "[parameters('addressSpaceSubnet')]"
                }
              }
            ]
          }
        }
      ]
    },
    "resourceGroup": "ResourceGroup2",
    "parameters": {
      "localPrefix": {
        "value": "[parameters('deployaVNETandonesubnet_localPrefix')]"
      },
      "addressSpaceVnet": {
        "value": "[parameters('deployaVNETandonesubnet_addressSpaceVnet')]"
      },
      "addressSpaceSubnet": {
        "value": "[parameters('deployaVNETandonesubnet_addressSpaceSubnet')]"
      }
    }
  }
}